<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Dmi.Domain" assembly="Dmi.Domain">
    <class name="SprintCalendar" table="[dbo].[SprintCalendar]" lazy="true" optimistic-lock="dirty" dynamic-update="true">
        <id name="RecordId">
            <generator class="native" />
        </id>
        
        <one-to-one name="Sprint" class="Sprint" constrained="true"/>
        
        <property name="StartDate" />
        <property name="StopDate" />

        <set name="CalendarDays" cascade="all-delete-orphan" inverse="true" lazy="true">
            <key column="SprintRecordId"/>
            <one-to-many class="SprintDay"/>
        </set>

    </class>
</hibernate-mapping>